void drawMap()
{
  for(int i=0;i<8;i++)
  {
    for(int j=0;j<6;j++)
    {
      GameTileStruct tile=Board1[j+sRowIdx][i+sColIdx];
      
      if(tile.player!=0)
      {
        if(tile.unitId!=0)
        {
          if(tile.active==1)
          {
            switch(tile.player)
            {
              case 1:
                gb.display.drawImage(i*10,j*10,IMAGE_PLAYER1_BOARD); break;
              case 2:
                gb.display.drawImage(i*10,j*10,IMAGE_PLAYER2_BOARD); break;
            }
          }
          else
          {
            gb.display.drawImage(i*10,j*10,IMAGE_DEACTIVATE_BOARD);
          }
          drawUnit(tile.unitId,i,j);
        }
      }
      else
      {
        if(tile.terrainTexture==11 || tile.terrainTexture==12) //Base/Building belonging
        {
          gb.display.drawImage(i*10,j*10,IMAGE_DEACTIVATE_BOARD);
          drawTerrain(tile.terrainTexture,i*10,j*10);
        }
        else
        {
          drawTerrain(tile.terrainTexture,i*10,j*10);
        }
      }
    }
  }
  gb.display.drawImage(posX,posY,IMAGE_CURSOR);
  if(unitMode==true)
  {
    gb.display.drawImage(selectedPosX,selectedPosY,CursorSelected);
  }
  //gb.display.setFontSize(1);
  //gb.display.setCursor(3,3);
  //gb.display.println(selectedUnit.unitName);
  debug();
}

void debug()
{
  gb.display.setColor(WHITE);
  gb.display.setCursor(3,3);
  gb.display.println(gb.getCpuLoad());
  gb.display.setCursor(3,9);
  gb.display.println(gb.getFreeRam());
}

